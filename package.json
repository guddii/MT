  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "20.9.3",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2"
  },
  "packageManager": "pnpm@8.9.0"
}