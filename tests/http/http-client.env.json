{
  "dev": {
    "tokenUrl": "http://proxy.localhost:4000/.oidc/token",
    "Security": {
      "Auth": {
        "client": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{clientId}}",
          "Client Secret": "{{clientSecret}}",
          "Token URL": "{{tokenUrl}}",
          "Scope": "webid"
        },
        "dpc": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{dpcId}}",
          "Client Secret": "{{dpcSecret}}",
          "Token URL": "{{tokenUrl}}",
          "Scope": "webid"
        }
      }
    }
  }
}